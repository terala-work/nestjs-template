    "@opentelemetry/sdk-trace-node": "^1.14.0",
    "@opentelemetry/semantic-conventions": "^1.14.0",
    "@opentelemetry/tracing": "^0.24.0",
    "aws-lambda": "^1.0.7",
    "aws-serverless-express": "^3.4.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "install": "^0.13.0",
    "nestjs-otel": "^5.1.2",
    "nestjs-zod": "^2.3.3",
