    "@nestjs/core": "^10.0.2",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.2",
    "@nestjs/swagger": "^7.0.2",
    "@opentelemetry/api": "^1.4.1",
