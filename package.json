    "@types/aws-serverless-express": "^3.3.5",
    "@types/express": "^4.17.17",
    "@types/hapi__joi": "^17.1.9",
    "@types/jest": "29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
